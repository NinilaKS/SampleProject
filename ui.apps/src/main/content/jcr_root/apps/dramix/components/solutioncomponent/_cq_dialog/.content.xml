<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="solution challenge"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-author_editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <solutionheading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="First Name"
                        name="./solutionheading"/>
                    <solutiondesc
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Last Name"
                        name="./solutiondesc"
                        required="{Boolean}false"/>
                    <well-multifield
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/well"
                        fieldLabel="Books">
                        <items jcr:primaryType="nt:unstructured">
                            <challengedetailswithmap
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Challenge Details"
                                required="{Boolean}false">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    emptyText="Books Details"
                                    name="./challengedetailswithmap">
                                    <items jcr:primaryType="nt:unstructured">
                                        <challengeheading
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="challenge Name"
                                            name="./challengeheading"/>
                                        <challengedesc
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="challenge desc"
                                            name="./challengedesc"/>
                                    </items>
                                </field>
                            </challengedetailswithmap>
                        </items>
                    </well-multifield>
                    <softwaremultifield
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/well"
                        fieldLabel="softwares">
                        <items jcr:primaryType="nt:unstructured">
                            <softwaredetailswithmap
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Software details"
                                required="{Boolean}false">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./softwaremultifield">
                                    <items jcr:primaryType="nt:unstructured">
                                        <softwares
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="softwareheading"
                                            name="./softwares"/>
                                        <desc
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="softwaredesc"
                                            name="./desc"/>
                                    </items>
                                </field>
                            </softwaredetailswithmap>
                        </items>
                    </softwaremultifield>
                    <temporheading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="temporheading"
                        name="./heading"/>
                    <tempordescr
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="tempordescr"
                        name="./headingdesc"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
